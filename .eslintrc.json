{
    "env": {
      "node": true,
      "commonjs": true,
      "es2021": true,
      "jest/globals": true
    },
    "extends": ["airbnb-base", "pretier", "plugin:jest/recommended"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "no-console": "off",
      "non-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "next"
        }
      ]
    }
  }